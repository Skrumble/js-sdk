{
  "name": "@skrumble/js-sdk",
  "version": "0.1.4",
  "description": "An SDK for Javascript SPAs to connect to the Skrumble API",
  "main": "dist/skrumble.min.js",
  "scripts": {
    "build": "webpack ; ./node_modules/.bin/jsdoc src/**/*.js -c ./jsdoc/jsdoc_conf.json",
    "docs": "./node_modules/.bin/jsdoc src/**/*.js -c ./jsdoc/jsdoc_conf.json",
    "docs_serve": "cd docs; python -m SimpleHTTPServer 5000",
    "test": "./node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/nyc --reporter=lcov --reporter=text --include=src/**/*.js --exclude=src/**/*.spec.js ./node_modules/.bin/mocha-webpack --webpack-config webpack.config.js \"test/**/*.spec.js\" -r jsdom-global/register --reporter spec"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "sdk",
    "Skrumble",
    "javascript"
  ],
  "author": "Skrumble Development Team",
  "license": "MIT",
  "devDependencies": {
    "axios-es6": "^0.11.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "browser-request": "^0.3.3",
    "chai": "^4.1.1",
    "cross-env": "^5.1.3",
    "glob": "^7.1.2",
    "jsdoc": "^3.5.4",
    "mocha": "^3.5.0",
    "nyc": "^11.6.0",
    "socket.io-client": "^2.0.4",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "ink-docstrap": "^1.3.0",
    "lodash": "^4.17.5",
    "mocha-webpack": "^0.7.0"
  }
}
